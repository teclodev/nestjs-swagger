{
  "name": "@teclo/nestjs-swagger-ui",
  "version": "0.1.0",
  "description": "An optimized NestJS module for rendering Swagger UI",
  "keywords": [
    "nestjs",
    "swagger"
  ],
  "author": "brendangoldberg",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --selectProjects unit",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "tidy": "run-s format lint",
    "prebuild": "rimraf dist",
    "paths": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp -r src/public dist/public"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "js-yaml": "^4.1.0",
    "swagger-ui-dist": "^5.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
